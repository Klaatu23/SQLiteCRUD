<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Principal">


    <ImageButton
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@mipmap/gift_icon_172437" android:id="@+id/imbt1"
            app:layout_constraintTop_toBottomOf="@+id/txt1" app:layout_constraintEnd_toStartOf="@+id/imbt2"
            android:layout_marginBottom="33dp" app:layout_constraintBottom_toTopOf="@+id/imbt3"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="34dp"
            android:layout_marginStart="34dp" android:layout_marginEnd="52dp" android:layout_marginRight="52dp"/>
    <ImageButton
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@mipmap/user" android:id="@+id/imbt2"
            android:layout_marginTop="286dp"
            app:layout_constraintStart_toEndOf="@+id/imbt1" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="31dp" app:layout_constraintBottom_toTopOf="@+id/imbt4"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="37dp"
            android:layout_marginRight="37dp"/>
    <ImageButton
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@mipmap/calendar" android:id="@+id/imbt3"
            app:layout_constraintTop_toBottomOf="@+id/imbt1" app:layout_constraintEnd_toStartOf="@+id/imbt4"
            android:layout_marginBottom="256dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="34dp" android:layout_marginStart="34dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="54dp"
            android:layout_marginRight="54dp"/>
    <ImageButton
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@mipmap/exit" android:id="@+id/imbt4"
            app:layout_constraintTop_toBottomOf="@+id/imbt2" app:layout_constraintStart_toEndOf="@+id/imbt3"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="254dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="40dp"
            android:layout_marginRight="40dp"/>
    <TextView
            android:text="Usuario"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/txt1"
            android:layout_marginTop="194dp"
            app:layout_constraintEnd_toStartOf="@+id/imbt2" android:layout_marginBottom="73dp"
            app:layout_constraintBottom_toTopOf="@+id/imbt1" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="21dp" android:layout_marginRight="21dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>